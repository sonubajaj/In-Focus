                                                  <!-- {% assign found = true %}<h3>My Heading Here</h3>-->
                                              {% endif %}
                                                    {{ tag | remove: attribute_keyword | replace_first: marker, ' '| capitalize | replace_first: ' ', '<div style="float: left" class="firsttext">' | prepend: ''  | append: '&#44;&nbsp;</div>' }}
                                                 {% endif %}
                                              {% if found and forloop.last %}{% endif %}
                                          {% endfor %}
                                  {% if area == blank %}
                                    <div>
                                      NA
                                  </div>
                                  {% endif %}
                               </div>
                            </div>
                            <!-- dealsin ends -->
                        <!-- about me panel ends -->
                      </div>
                     <!-- left panel ends -->
                     <!-- right panel starts -->
                      <div style="width: 320px; min-height: 300px; float: left;">
                          <!-- top content -->
                          <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px; min-height: 150px;">
                             <div style="float: left; width: 150px; min-height: 150px;">
                               	 <div>
                                   <div class="professional-heading" style="margin-top: 5px; margin-left: 5px;">
