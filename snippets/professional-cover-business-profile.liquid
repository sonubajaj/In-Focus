<a name="businessprofile">
            	<div style="width: 698px; min-height: 220px;">
                   <div class="professional-main-heading">
                     <i class="fa fa-black-tie" style="color:#f90b0e"></i>&nbsp;&nbsp;Business Profile
                    </div>
                   <!-- about content starts -->
                   <div style="border:1px solid #e0e0e0; min-height: 320px; margin-bottom: 10px; margin-top: 10px;">
                     <!-- left panel starts -->
                      <div style="width: 370px; border-right: 1px solid #e0e0e0; min-height: 320px; float: left;">
                          <!-- about me panel -->
                            <div style="padding: 10px; min-height: 50px;">
                               <div class="professional-heading" style="margin-top: 5px; margin-left: 5px;">
                                   About my Business
                               </div>
                               <div style="margin-left: 8px; margin-top: 5px; min-height: 70px;" class="firsttext professional-content" class="">
                                  {% if businessinfo != blank %}
                            		{{ businessinfo }}
                                  {% endif %}
                                  {% if businessinfo == blank %}
                                    <div>
                                      NA
                                  </div>
                                  {% endif %}
                               </div>
                            </div>
                            <!-- dealin starts -->
                            <div style="clear: both; padding: 5px; min-height: 30px;">
                               <div class="professional-heading" style="margin-top: 5px; margin-left: 10px;">
                                   Deals In
                               </div>
                               <div style="margin-left: 13px; margin-top: 5px; min-height: 45px;" class="professional-content">
                                  {% assign attribute_keyword = 'dealsin:' %}
                                          {% assign marker = ':' %}
                                          {% assign found = false %}
                                          {% for tag in product.tags %}
                                              {% if tag contains attribute_keyword %}
                                                {% if found == false %}
                                                  <!-- {% assign found = true %}<h3>My Heading Here</h3>-->
                                              {% endif %}
                                                    {{ tag | remove: attribute_keyword | replace_first: marker, ' '| capitalize | replace_first: ' ', '<div style="float: left" class="firsttext">' | prepend: ''  | append: '&#44;&nbsp;</div>' }}
                                                 {% endif %}
                                              {% if found and forloop.last %}{% endif %}
                                          {% endfor %}
                                  {% if dealsin == blank %}
                                    <div>
                                      NA
                                  </div>
                                  {% endif %}
                               </div>
                            </div>
                            <!-- dealsin ends -->
                           <!-- dealin starts -->
                            <div style="clear: both; padding: 5px; min-height: 30px;">
                               <div class="professional-heading" style="margin-top: 5px; margin-left: 10px;">
                                   Area we Deals In
                               </div>
                               <div style="margin-left: 13px; margin-top: 5px; min-height: 40px;" class="firsttext professional-content">
                                  {% assign attribute_keyword = 'adi:' %}
                                          {% assign marker = ':' %}
                                          {% assign found = false %}
                                          {% for tag in product.tags %}
                                              {% if tag contains attribute_keyword %}
