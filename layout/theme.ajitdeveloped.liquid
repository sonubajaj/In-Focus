<!doctype html>
<!--[if lt IE 7 ]> <html lang="en" class="no-js ie6"> <![endif]-->
<!--[if IE 7 ]>    <html lang="en" class="no-js ie7"> <![endif]-->
<!--[if IE 8 ]>    <html lang="en" class="no-js ie8"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html lang="en" class="no-js"> <!--<![endif]-->
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <link rel="canonical" href="{{ canonical_url }}" />

  
  {% comment %}
    Provide a *slightly* better product description for search engines.
    Descriptions don't affect rankings, but can influence click-throughs.
  {% endcomment %}

  {% if page_description %}
  <meta name="description" content="{{ page_description }}" />
  {% endif %}

  {% include 'open-graph' %}
  {% include 'twitter-cards' %}

  <title>
    {% if template == 'index' %}{% if page_title contains 'Welcome' %}{{ shop.name }} &ndash; Welcome{% else %}{{ page_title }}{% endif %}{% elsif template == '404' %}Page Not Found{% elsif page_title contains shop.name %}{{ page_title }}{% else %}{{ page_title }} &ndash; {{ shop.name }}{% endif %}
  </title>

  {{ content_for_header }}
  <link rel="shortcut icon" href="{{'favicon.ico' | asset_url}}" type="image/x-icon" >
  {% if settings.header_font == "Chewy, arial, sans-serif" %}
    <link href='//fonts.googleapis.com/css?family=Chewy' rel='stylesheet' type='text/css'>
  {% elsif settings.header_font == "Copse, arial, sans-serif" %}
    <link href='//fonts.googleapis.com/css?family=Copse' rel='stylesheet' type='text/css'>
  {% elsif settings.header_font == "Play, arial, sans-serif" %}
    <link href='//fonts.googleapis.com/css?family=Play' rel='stylesheet' type='text/css'>
  {% elsif settings.header_font == "Arvo, arial, sans-serif" %}
    <link href='//fonts.googleapis.com/css?family=Arvo' rel='stylesheet' type='text/css'>
  {% endif %}

  {{ 'style.css' | asset_url | stylesheet_tag }}
  {{ 'custom.css' | asset_url | stylesheet_tag }}
  {{ 'colorbox.css' | asset_url | stylesheet_tag }}
  {{ 'facebox.css' | asset_url | stylesheet_tag }}
    {{ 'Mycss.css' | asset_url | stylesheet_tag }}
   {{ 'font-awesome.css' | asset_url | stylesheet_tag }}
  {{ 'product-reviews-override.css' | asset_url | stylesheet_tag }}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}

  {% if template contains 'customers' %}
    {{ "shopify_common.js" | shopify_asset_url | script_tag }}
    {{ "customer_area.js"  | shopify_asset_url | script_tag }}
  {% endif %}

   
  {% include 'analytics' %}
</head>

<body class="{% if customer %}customer-logged-in{% endif %} template-{{ template | replace: '.', ' ' | replace: '/', '-' | truncatewords: 1, '' }}" id="{{ page_title | handle }}">
  <!-- pop up content starts 
  {% include 'homepage-popup-content' %}
  <!-- popup content ends -->
  <!-- Header -->
  
  <div id="divnewheader">
    {% include 'NewHeader' %}
  
  </div>


  <div id="main-wrapper">

   


    <!-- Main Content -->
      <!-- banner panel starts -->
    <div style="height: 470px; margin-top: -10px;">
      <div class="banner-panel-professional" id="display_image">
      <div class="row clearfix">

        {% if template == 'list-collections' %}
          {% include 'collection-listing' %}
        {% else %}
          {{ content_for_layout }}
          <!-- main banner starts -->
                <div id="main" class="wrapper clearfix">
          		{% include 'index-banner' %}
                </div>
          <!-- main banner ends -->
        {% endif %}

      </div>
      </div>
    </div>
    <!-- banner panel ends  -->
    <!-- main content panel starts -->
    <div class="row clearfix" style="background-color:#f1f1f1;">
    <div id="main" class="wrapper clearfix">
     <!-- info panel starts -->
      	{% include 'info-counter-panel' %}
     <!-- info panel ends -->
      </div>
      </div>
      <div class="row clearfix" style="background-color:#fff;">
      <div id="main" class="wrapper clearfix">
      <!-- get property advice panel starts -->
         {% include 'get-property-advice' %}
      <!-- get property advice panel ends -->
      </div>
      </div>
      <div class="row clearfix" style="background-color:#f1f1f1;">
      <div id="main" class="wrapper clearfix">
      <!-- why us starts -->
         {% include 'why-us-index' %}
      <!-- why us ends -->
      </div>
      </div>
      <div class="row clearfix" style="background-color:#fff;">
      <div id="main" class="wrapper clearfix">
      <!-- why us starts -->
        {% include 'new-projects-index' %}
      <!-- why us ends -->
        <div style="clear: both;">
        </div>
        <!-- new project banner starts -->
        <div style="width: 100%; height: 90px; margin-top: 20px; margin-bottom: 20px;">
           <div id=container class=banner>
            <ol>
              <li><a href="http://in-focus.in/pages/apnaghar"><img src="{{'apna_banner1.jpg' | asset_url}}" style="width: 100%; border-radius: 5px;"></a></li>
                </ol>
              <div class=mouse_direction>
                  <div class=mouse_top></div>
                  <div class=mouse_bottom></div>
              </div>
          </div>
        </div>
        
        <!-- new projects banner ends -->
        <div style="clear: both;">
        </div>
      </div>
      </div>
     </div>
    <!-- main content panel ends -->
    
    <!-- /#main -->
  <!-- /#main-wrapper -->


  <!-- Footer Starts -->
  {% include 'footer' %}
  <!-- Footer Ends -->
</body>
</html>
